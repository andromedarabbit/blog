{
    {{ range .Site.RegularPages }}
    "{{ .RelPermalink }}": {
        "full_url": "{{ .Permalink }}",
        "kind": "{{ .Kind }}",
        "date": "{{ .Date.Format "2006-01-02" }}",
        "source_path": {{ .File.Path |jsonify }},
        {{ if .Params.syndicated }}
        "syndicated": [
            {{ range .Params.syndicated }}
            {
                "url": "{{ .url }}",
                "type": "{{ .type }}"
            },
            {{ end }}
            {}
        ],
        {{ end }}
        {{ if .Params.link }}
        "link": {{ .Params.link.url |jsonify }},
        {{ end }}
        "title": {{ .Title | jsonify }}
    },
    {{ end }}
    "Dummy": {}
}